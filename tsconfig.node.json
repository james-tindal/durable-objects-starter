{
	"compilerOptions": {
		"strict": true,
		"noImplicitAny": false,
		"module": "esnext",
		"target": "esnext",
		"lib": ["esnext"],
		"moduleResolution": "bundler",
		"noEmit": true,
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"strictPropertyInitialization": false,
		"paths": {
			"/*": ["./*"]
		}
	}
}
